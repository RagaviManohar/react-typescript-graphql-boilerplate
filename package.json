{
  "name": "react-typescript-graphql-boilerplate",
  "description": "Terapool Admin Management",
  "version": "2.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-boilerplate/react-boilerplate-cra-template.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.x"
  },
  "bugs": {
    "url": "https://github.com/react-boilerplate/react-boilerplate-cra-template/issues"
  },
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "keywords": [
    "react",
    "create-react-app",
    "template",
    "typescript",
    "redux",
    "reduxjs",
    "react-redux",
    "react-boilerplate"
  ],
  "scripts": {
    "// ---DEFAULT CRA---": "",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "// ---TEMPLATE---": "Scripts will be also in the template",
    "start:prod": "npm run build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "npm run eslint src",
    "lint:fix": "npm run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "// ---TESTING TEMPLATE---": "",
    "test:internals": "npm run test:extract-messages && npm run test:generators",
    "test:extract-messages": "jest --config=internals/extractMessages/jest.config.js",
    "test:cov": "npm run test --watchAll=false -- --coverage",
    "test:cra": "npm run create:cra-app && cd generated-cra-app && npm run test:generators && npm run lint && npm run checkTs && npm run cleanAndSetup && npm run lint && npm run checkTs",
    "// ---BUILDING TEMPLATE---": "",
    "create:npm-package": "ts-node ./internals/scripts/create-npm-package.script.ts",
    "create:cra-app": "ts-node ./internals/scripts/create-cra-app.script.ts",
    "// ---PUBLISHING---": "",
    "publish:github": "cross-env PUBLIC_URL='/react-boilerplate-cra-template' npm build && gh-pages -d build",
    "changelog": "ts-node ./internals/scripts/create-changelog.script.ts",
    "release": "standard-version",
    "publish:npm": "npm run create:npm-package && npm publish .cra-template-rb --non-interactive"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.ts",
      "!src/**/*/messages.ts",
      "!src/reportWebVitals.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleNameMapper": {
      "^jest.setup": "<rootDir>/jest.setup"
    },
    "modulePathIgnorePatterns": [
      "__fixtures__"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@apollo/react-testing": "4.0.0",
    "@date-io/dayjs": "2.16.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.10.9",
    "@mui/lab": "5.0.0-alpha.106",
    "@mui/material": "5.10.12",
    "@mui/x-date-pickers": "5.0.6",
    "@reduxjs/toolkit": "1.8.6",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "dayjs": "1.11.6",
    "fontfaceobserver": "2.3.0",
    "graphql": "16.6.0",
    "i18next": "22.0.4",
    "i18next-browser-languagedetector": "7.0.0",
    "node-sass": "6.0.1",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-hook-form": "7.39.1",
    "react-i18next": "12.0.0",
    "react-is": "18.2.0",
    "react-redux": "8.0.4",
    "react-router-dom": "6.4.3",
    "react-scripts": "5.0.1",
    "redux-injectors": "2.0.0",
    "redux-saga": "1.2.1",
    "sanitize.css": "13.0.0",
    "stream-browserify": "3.0.0",
    "ts-invariant": "0.10.3",
    "util": "0.12.5",
    "web-vitals": "3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/fontfaceobserver": "2.1.0",
    "@types/jest": "29.2.1",
    "@types/lodash": "4.14.187",
    "@types/node": "18.11.9",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "18.0.0",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.11",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.18.0",
    "chalk": "5.1.2",
    "cross-env": "7.0.3",
    "devmoji": "2.3.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "4.0.0",
    "git-branch-is": "4.0.0",
    "husky": "8.0.1",
    "i18next-scanner": "4.1.0",
    "inquirer": "8.2.4",
    "inquirer-directory": "2.2.0",
    "lint-staged": "13.0.3",
    "node-plop": "0.31.0",
    "plop": "2.7.4",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "replace-in-file": "6.3.5",
    "rimraf": "3.0.2",
    "serve": "14.0.1",
    "shelljs": "0.8.5",
    "standard-version": "9.5.0",
    "stylelint": "14.14.0",
    "stylelint-config-recommended": "9.0.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
